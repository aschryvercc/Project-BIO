<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_ICSVExportService" />
                <binding name="BasicHttpBinding_IqbExportService" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost/CSVExportService/CSVExportService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICSVExportService"
                contract="CSVExportService.ICSVExportService" name="BasicHttpBinding_ICSVExportService" />
            <endpoint address="http://localhost/qbExportService/qbExportService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IqbExportService"
                contract="qbExportService.IqbExportService" name="BasicHttpBinding_IqbExportService" />
        </client>
    </system.serviceModel>
</configuration>